{
	"extends": ["next", "next/core-web-vitals", "prettier"],
	"plugins": ["react", "@typescript-eslint", "unused-imports"],
	"rules": {
		"unused-imports/no-unused-imports": "error",
		"unused-imports/no-unused-vars": [
			"warn",
			{
				"vars": "all",
				"varsIgnorePattern": "^_",
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		]
	}
}
